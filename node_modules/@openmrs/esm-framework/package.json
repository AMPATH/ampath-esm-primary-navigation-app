{
  "name": "@openmrs/esm-framework",
  "version": "8.0.1-pre.3705",
  "license": "MPL-2.0",
  "type": "module",
  "module": "dist/openmrs-esm-framework.js",
  "types": "src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./src/internal": {
      "types": "./src/internal.ts",
      "default": "./src/internal.ts"
    },
    "./package.json": {
      "default": "./package.json"
    },
    "./mock": {
      "import": "./mock.tsx",
      "require": "./mock-jest.tsx"
    },
    "./mock.tsx": {
      "import": "./mock.tsx",
      "require": "./mock-jest.tsx"
    }
  },
  "source": true,
  "scripts": {
    "document": "../../../document.sh esm-framework",
    "test": "cross-env TZ=UTC vitest run --passWithNoTests",
    "test:watch": "cross-env TZ=UTC vitest watch",
    "coverage": "cross-env TZ=UTC vitest run --coverage --passWithNoTests",
    "build": "concurrently \"rspack --mode=production\" \"tsc --project tsconfig.build.json\"",
    "build:development": "concurrently \"rspack --mode=development\" \"tsc --project tsconfig.build.json\"",
    "analyze": "rspack --mode=production --env analyze=true",
    "typescript": "tsc --project tsconfig.build.json",
    "lint": "eslint src --ext ts,tsx"
  },
  "keywords": [
    "openmrs",
    "framework",
    "frontend",
    "ui",
    "microfrontends"
  ],
  "browserslist": [
    "extends browserslist-config-openmrs"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/openmrs/openmrs-esm-core.git"
  },
  "bugs": {
    "url": "https://github.com/openmrs/openmrs-esm-core/issues"
  },
  "homepage": "https://github.com/openmrs/openmrs-esm-core#readme",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@openmrs/esm-api": "8.0.1-pre.3705",
    "@openmrs/esm-config": "8.0.1-pre.3705",
    "@openmrs/esm-context": "8.0.1-pre.3705",
    "@openmrs/esm-dynamic-loading": "8.0.1-pre.3705",
    "@openmrs/esm-emr-api": "8.0.1-pre.3705",
    "@openmrs/esm-error-handling": "8.0.1-pre.3705",
    "@openmrs/esm-expression-evaluator": "8.0.1-pre.3705",
    "@openmrs/esm-extensions": "8.0.1-pre.3705",
    "@openmrs/esm-feature-flags": "8.0.1-pre.3705",
    "@openmrs/esm-globals": "8.0.1-pre.3705",
    "@openmrs/esm-navigation": "8.0.1-pre.3705",
    "@openmrs/esm-offline": "8.0.1-pre.3705",
    "@openmrs/esm-react-utils": "8.0.1-pre.3705",
    "@openmrs/esm-routes": "8.0.1-pre.3705",
    "@openmrs/esm-state": "8.0.1-pre.3705",
    "@openmrs/esm-styleguide": "8.0.1-pre.3705",
    "@openmrs/esm-translations": "8.0.1-pre.3705",
    "@openmrs/esm-utils": "8.0.1-pre.3705"
  },
  "peerDependencies": {
    "dayjs": "1.x",
    "i18next": "25.x",
    "react": "18.x",
    "react-dom": "18.x",
    "react-i18next": "16.x",
    "rxjs": "6.x",
    "single-spa": "6.x",
    "swr": "2.x"
  },
  "devDependencies": {
    "@openmrs/typedoc-plugin-file-categories": "8.0.1-pre.3705",
    "@rspack/cli": "^1.3.11",
    "@rspack/core": "^1.3.11",
    "@vitest/coverage-v8": "^4.0.7",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.13",
    "happy-dom": "^17.4.7",
    "sass-embedded": "^1.89.0",
    "svgo-loader": "^4.0.0",
    "typedoc": "^0.28.5",
    "typedoc-plugin-markdown": "^4.6.3",
    "typedoc-plugin-no-inherit": "^1.6.1",
    "typescript": "^5.8.3",
    "vitest": "^4.0.7"
  },
  "stableVersion": "8.0.0"
}